{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.0.0",
        "id": "example-declaration-01",
        "label": "Sample HTTP/HTTPS application",
        "remark": "HTTP/HTTPS application deployment",
        "Sample_01": {
            "class": "Tenant",
            "defaultRouteDomain": 0,
            "Application_1": {
                "class": "Application",
                "template": "https",
                "serviceMain": {
                    "class": "Service_HTTPS",
                    "virtualAddresses": [
                        "10.1.10.10"
                    ],
                    "pool": "web_pool",
                    "serverTLS": {
                        "bigip": "/Common/clientssl"
                    },
                    "snat": "self",
                    "persistenceMethods": [
                    	"source_address"
                    ]
                },
                "web_pool": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 8080,
                            "serverAddresses": [
                                "192.168.128.11",
                                "192.168.128.12",
                                "192.168.128.13"
                            ]
                        }
                    ]
                }
            }
        }
    }
}